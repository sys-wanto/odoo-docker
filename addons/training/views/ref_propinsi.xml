      <field name="model">ctu.ref_propinsi</field>
      <field name="arch" type="xml">
        <tree>
          <field name="kd_propinsi" />
          <field name="nm_propinsi" />

        </tree>
      </field>
    </record>

    <!-- ctu.ref_propinsi form view -->
    <record id="ctu_ref_propinsi_view_form" model="ir.ui.view">
      <field name="name">ctu.ref_propinsi.view.form</field>
      <field name="model">ctu.ref_propinsi</field>
      <field name="arch" type="xml">
        <form string="">
          <sheet>
            <group>
              <field name="kd_propinsi" />
              <field name="nm_propinsi" />

            </group>
          </sheet>
        </form>
      </field>
    </record>

    <!-- ctu.ref_propinsi action window -->
    <record id="ctu_ref_propinsi_action" model="ir.actions.act_window">
      <field name="name">Class</field>
      <field name="type">ir.actions.act_window</field>
      <field name="res_model">ctu.ref_propinsi</field>
      <field name="view_mode">tree,form,pivot,graph</field>
      <field name="help" type="html">
        <p class="oe_view_nocontent_create">
          <!-- Add Text Here -->
        </p>
        <p>
          <!-- More details about what a user can do with this object will be OK -->

        </p>
